# Autoppia Worker Configuration
# Copy this file to .env and fill in your values

# AI Provider Configuration
# Chutes API is the primary AI provider (recommended)
CHUTES_API_KEY=your_chutes_api_key_here
CHUTES_API_URL=https://api.chutes.ai

# Optional: Only needed if you want direct OpenAI/Anthropic integration
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here

# Worker Configuration
WORKER_NAME=autoppia-miner
WORKER_VERSION=0.1.0
WORKER_DESCRIPTION=An Autoppia AI Worker for mining and processing tasks

# Logging
LOG_LEVEL=INFO

